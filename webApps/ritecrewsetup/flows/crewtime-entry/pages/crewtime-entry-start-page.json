      "equipment_category": "string",
      "equipment_rate": "string",
      "ext_time_diff_flag": "string",
      "fri_in_time": "string",
      "fri_out_time": "string",
      "fri_total_hours": "number",
      "hours_validation": "string",
      "input_date": "any",
      "installed_quantity": "any",
      "location": "any",
      "millage": "any",
      "mon_in_time": "string",
      "mon_out_time": "string",
      "mon_total_hours": "number",
      "ot_rate": "string",
      "pay_rate": "string",
      "per_diem_amount": "any",
      "per_diem_rate": "string",
      "perdiem_flag": "string",
      "perdiem_quantity": "any",
      "phase": "any",
      "po": "any",
      "po_line": "any",
      "primary_timekeeper_id": "number",
      "project_id": "number",
